#!/Users/ufuk.dogan/Master_Thesis/virtual_env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'parlai','console_scripts','parlai'
__requires__ = 'parlai'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('parlai', 'console_scripts', 'parlai')()
    )
